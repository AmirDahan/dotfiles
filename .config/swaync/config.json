{
  "$schema": "/etc/xdg/swaync/configSchema.json",
  "positionX": "center",
  "positionY": "top",
  "layer": "top",
  "cssPriority": "application",
  "control-center-margin-top": 5,
  "control-center-margin-bottom": 0,
  "control-center-margin-right": 1510,
  "control-center-margin-left": 0,
  "notification-icon-size": 64,
  "notification-body-image-height": 128,
  "notification-body-image-width": 200,
  "timeout": 10,
  "timeout-low": 5,
  "timeout-critical": 0,
  "fit-to-screen": true,
  "control-center-width": 400,
  "control-center-height": 650,
  "notification-window-width": 375,
  "keyboard-shortcuts": true,
  "image-visibility": "when-available",
  "transition-time": 200,
  "hide-on-clear": true,
  "hide-on-action": true,
  "script-fail-notify": true,
  "scripts": {
    "example-script": {
      "exec": "mpv --no-video /usr/share/sounds/freedesktop/stereo/message.oga",
      "urgency": "Normal"
    }
  },
  "widgets": [
    "title",
    "buttons-grid",
    "mpris",
    "volume",
    "notifications"
  ],
  "widget-config": {
    "title": {
      "text": "Quick settings",
      "clear-all-button": true,
      "button-text": ""
    },
    "volume": {
      "label": ""
    },
    "dnd": {
      "text": "Do Not Disturb"
    },
    "mpris": {
      "image-size": 96,
      "image-radius": 8
    },
    "buttons-grid": {
      "actions": [
        {
          "label": "",
          "command": "~/.config/scripts/swaync.sh 1"
        },
        {
          "label": "",
          "command": "~/.config/scripts/swaync.sh 2"
        },
        {
          "label": "",
          "command": "~/.config/scripts/swaync.sh 3"
        }
      ]
    }
  }
}
